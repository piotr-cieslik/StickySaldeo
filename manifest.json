{
  "name": "sticky saldeo",
  "version": "1.0",
  "description": "It makes document preview sticky to the top.",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://saldeo.brainshare.pl/*",
        "https://www.saldeo.brainshare.pl/*"],
      "js": ["contentScript.js"]
    }
  ]
}